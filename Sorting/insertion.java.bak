import java.util.*;
class  insertion
{
	public static void main(String[] args) 
	{
		int a[]={2,6,7,0,34,1,987,3,67,45,298,76};
		int result[]=insert(a);
		System.out.println(Arrays.toString(result));	
	}
	public static int[] insert(int a[])
	{
		for(int i=1;i<a.length;i++)
		{
			int key=a[i];
			int j=i-1;
			while(j>=0 && a[j]>key)
			{
				a[j+1]=a[j];
			}
			a[j+1]=key;
		}
		return a;
	}
}
