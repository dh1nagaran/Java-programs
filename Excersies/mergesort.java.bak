import  java.util.*;
class quick
{
	public static void main(String[] args)
	{
		int a[]= {2,1,65, 23, 13, 18, 30, 46, 17, 52, 78};
		int n=a.length;
		int mid=n;
		while(mid>0)
		{
			mid=mid/2;
			a=solve(a,mid);
		}
		System.out.println(mid);
		System.out.println(Arrays.toString(a));
		
	}
	public static int[] solve(int a[],int mid)
	{
		for(int i=0;i<mid;i++)
		{
			if(a[i]>a[i+1])
			{
				int temp=a[i];
				a[i]=a[i+1];
				a[i+1]=temp;	
			}	
		}
		for(int j=mid;j<a.length-1;j++)
		{
			if(a[j]>a[j+1])
			{
				int temp2=a[j];
				a[j]=a[j+1];
				a[j+1]=temp2;
			}
			
		}
		
		return a;
	}
}